event LaunchAgent_Add:
    key:
        event_src.host
    filter {
        filter::NotFromCorrelator()
        and event_src.title == "edr"
        and msgid == "9"
        and action == "execute"
        and lower(object.process.fullpath) == "/bin/launchctl"
        and (
            regex(lower(object.process.cmdline), ".*(launchctl\s+load)\s+(.*\s+)?(.*?)/library/launchagents/(.*\.plist)", 0) != null
            )
    }

event Script_Execute:
    key:
        event_src.host
    filter {
        filter::NotFromCorrelator()
        and event_src.title == "edr"
        and msgid == "9"
        and action == "execute"
        and lower(subject.process.fullpath) == "/usr/libexec/xpcproxy"
        and (
            regex(lower(datafield17), "com\.apple\.(sh|bash|zsh|ksh|dash|tcsh|tclsh)", 0) != null
            or regex(lower(object.process.cmdline), "(\/bin\/(bash|sh|zsh|ksh|dash))|(\/usr\/(local\/)?bin\/(env\s+python|python[0-9\.]*|perl|ruby|php|awk|sed|osascript|javascript|java|curl|wget|tcsh|tclsh|wish))\s+.*", 0) != null
            )
    }

